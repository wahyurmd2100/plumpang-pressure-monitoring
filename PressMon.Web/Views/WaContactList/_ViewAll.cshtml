@model TMS.Web.Models.WaContactList
<div class="table-responsive">
    <table id="tb-contact-list" class="table table-striped table-bordered dt-responsive nowrap table-hover" cellspacing="0">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ContactID)
                </th>
                <th>Action</th>
                <th>
                    @Html.DisplayNameFor(model => model.ContactName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ContactNumber)
                </th>
            </tr>
        </thead>
    </table>
</div>

